{#simple example for terminal IRTG rule generation#}
{= 

consBaseTemp, CBT : Temp := {|<@e;t;t app:layout_constraint{$ what $}_to{$ whatTo $}Of="{$ whatOf $}"|}
CTDT, ConsTypeDataType :Type	:= what, whatTo :Text
WODT, WhatOfDataType :Type	:= whatOf :Text

consListTypeTT,   CL3T : Temp   := <|<$stst;enst;toto 
consListTypeTB,	  CLTTB : Temp  := <|<$stst;enst;tobo 
consListTypeTBBB, CLTT3B : Temp := <|<$stst;enst;tobo;bobo 
CLDT, consListDataType :Type	:= stst, sten, enst, enen, toto, tobo, boto, bobo :Temp

buttBase,BB : Temp := 
	{|
		<@e;t <Button
		<@e;t;t android:text="@string/{$ textID $}"
		<@e;t;t android:id="@+id/button{$ idNumber $}"
		<@e;t;t android:layout_width="124dp"
		<@e;t;t android:layout_height="wrap_content"
		<@e;e;t;t android:background="@drawable/mybutton"
		<@e;e;t;t android:layout_marginTop="{$ marginT $}dp"
		<@e;t;t android:layout_marginBottom="{$ marginB $}8dp"
		<@e;e;t;t app:layout_constraintVertical_bias="{$ biasV $}"
		<@e {$cons$}
	|}
BDT,buttDataType : Type  := textID ,idNumber, marginT, marginB, biasV : Text, cons : Temp

consListDatas, CLD :CLDT	:= 
	 [+CBT.copy :+ [=:CTDT:= <"Start 	,<"Start
	,[+CBT.copy :+ [=:CTDT:= <"Start 	,<"End
	,[+CBT.copy :+ [=:CTDT:= <"End 		,<"Start
	,[+CBT.copy :+ [=:CTDT:= <"End 		,<"End
	,[+CBT.copy :+ [=:CTDT:= <"Top 		,<"Top
	,[+CBT.copy :+ [=:CTDT:= <"Top 		,<"Bottom
	,[+CBT.copy :+ [=:CTDT:= <"Bottom 	,<"Top
	,[+CBT.copy :+ [=:CTDT:= <"Bottom 	,<"Bottom
consDataList, CDL : List:Temp :=
	 [+ <+CL3T:+CLD		:+ <=:List:WODT:={=:WODT:= <"@+id/guideline =},{=:WODT:= <"@+id/guideline =},{=:WODT:= <"parent =} :0:+0,1:+1,2:+2
	,[+ <+CLTTB:+CLD	:+ <=:List:WODT:={=:WODT:= <"@+id/guideline =},{=:WODT:= <"@+id/guideline =},{=:WODT:= <"@+id/button1 =} :0:+0,1:+1,2:+2
	,[+ <+CLTT3B:+CLD	:+ <=:List:WODT:={=:WODT:= <"@+id/guideline =},{=:WODT:= <"@+id/guideline =},{=:WODT:= <"@+id/button2 =},{=:WODT:= <"parent =} :0:+0,1:+1,2:+2,3:+3
	,[+ <+CL3T:+CLD		:+ <=:List:WODT:={=:WODT:= <"@+id/guideline2 =},{=:WODT:= <"@+id/guideline2 =},{=:WODT:= <"parent =} :0:+0,1:+1,2:+2
	,[+ <+CLTTB:+CLD	:+ <=:List:WODT:={=:WODT:= <"@+id/guideline2 =},{=:WODT:= <"@+id/guideline2 =},{=:WODT:= <"@+id/button4 =} :0:+0,1:+1,2:+2
	,[+ <+CLTT3B:+CLD	:+ <=:List:WODT:={=:WODT:= <"@+id/guideline2 =},{=:WODT:= <"@+id/guideline2 =},{=:WODT:= <"@+id/button5 =},{=:WODT:= <"parent =} :0:+0,1:+1,2:+2,3:+3
buttDatas, BD : List:BDT :=
	 [=:BDT:= <"contine 	,<"1	,<"176	,<"0	,<"1	,CDL.0
	,[=:BDT:= <"connect 	,<"2	,<"52	,<"0	,<"1	,CDL.1
	,[=:BDT:= <"achivements 	,<"3	,<"52	,<"8	,<"0.03	,CDL.2
	,[=:BDT:= <"new_game 	,<"4	,<"176	,<"0	,<"1	,CDL.3
	,[=:BDT:= <"options 	,<"5	,<"52	,<"0	,<"1	,CDL.4
	,[=:BDT:= <"records 	,<"6	,<"52	,<"0	,<"0.03	,CDL.5
=}
